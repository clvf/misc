alias ps='ps -ww f'
alias psa='ps -e'
alias diff='diff -ubwt'
alias gsync='rsync -L -r --exclude=.git --exclude=build_utils --exclude=.pytest_cache --progress --human-readable'
alias ica='/opt/Citrix/ICAClient/wfica -icaroot /opt/Citrix/ICAClient'
alias ipython='python -mIPython'
alias lesc='less -SC'
alias ll='ls -alF'
alias llt='ls -altr'
alias ls='ls --color -F'
alias od='od -A o -t co1 -v'
alias pstree='pstree -a -l -u -U -p'
alias piconn='sudo screen /dev/ttyUSB0 115200'
alias pycrm="find . -follow -depth -name '*__pycache__' -type d -exec rm -rf {} \;"
alias view='vim -R'
alias vim='vim -p'
alias yapf='/usr/bin/yapf3 --style $HOME/.style.yapf'
alias yapfi='yapf --in-place'
alias tmux='tmux -u'
